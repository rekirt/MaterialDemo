<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lc.materialdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CoordinatorLayoutActivity" android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".ActionBarActivity" android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".DrawerWithToolBarActivity1" android:theme="@style/AppThemeNoTitle">
        </activity>
        <activity android:name=".DrawerWithToolBarActivity2" android:theme="@style/AppThemeNoTitle">
        </activity>
    </application>

</manifest>